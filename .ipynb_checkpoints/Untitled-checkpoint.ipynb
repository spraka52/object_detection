   "source": [
    "import os \n",
    "import sys \n",
    "args = sys.argv \n",
    "directory = args[1] \n",
    "protoc_path = args[2] \n",
    "for file in os.listdir(directory):\n",
    "    if file.endswith(\".proto\"):\n",
    "        os.system(protoc_path+\" \"+directory+\"/\"+file+\" --python_out=.\")"
   ]
  }
